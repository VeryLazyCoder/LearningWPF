<Application x:Class="LearningWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LearningWPF"
             xmlns:commands="clr-namespace:LearningWPF.Infrastructure"
             StartupUri="Views/RegisterWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <commands:CloseApplicationCommand x:Key="CloseCommand"/>
            <commands:SwitchToMenuWindowCommand x:Key="SwitchToMenuCommand"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
